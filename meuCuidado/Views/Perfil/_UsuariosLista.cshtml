@model IList<meuCuidado.Dominio.Models.Usuario>

<div class="row">
    @foreach (var usuario in Model)
    {
        <div class="col-md-3">
            <div class="card" style="margin: 10px;">
                <img src="https://i.pinimg.com/564x/ac/0f/41/ac0f419e977af681516e00829c5393ee.jpg" alt="Descrição da imagem" class="card-img-top" style="max-width:100%; height:auto;" />
                <div class="card-body">
                    <h5 class="card-title">@usuario.Nome</h5>
                    <p class="card-text">@usuario.Email</p>
                </div>
            </div>
        </div>
    }
</div>
